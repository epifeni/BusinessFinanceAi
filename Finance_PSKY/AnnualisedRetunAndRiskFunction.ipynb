   "outputs": [],
   "source": [
    "def ann_risk_retun(returns_df):\n",
    "    summary = returns_df.agg(['mean', 'std']).T\n",
    "    summary.columns = ['Return', 'Risk']\n",
    "    summary.Return = summary.Return * 252\n",
    "    summary.Risk = summary.Risk * np.sqrt(252)\n",
    "    return summary"
   ]
  }
